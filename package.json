{
  "name": "facsimile",
  "version": "1.0.0",
  "description": "A utility to deep copy any JavaScript object",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --exit",
    "coverage": "cross-env NODE_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryandabler/facsimile.git"
  },
  "author": "Ryan Dabler <r.p.dabler@gmail.com> (https://ryandabler.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryandabler/facsimile/issues"
  },
  "homepage": "https://github.com/ryandabler/facsimile#readme",
  "dependencies": {
    "tupos": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-minify": "^0.4.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2"
  },
  "babel": {
    "presets": [
      [ "@babel/preset-env", { "targets": { "node": "current" } } ]
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      },
      "production": {
        "presets": [
          "minify"
        ]
      }
    }
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "require": [
      "@babel/plugin-transform-regenerator",
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "text",
      "lcov"
    ]
  }
}
